{
  "name": "valyrian",
  "version": "1.0.0",
  "description": "Valyrian",
  "scripts": {
    "start": "webpack-dev-server --color --inline --progress --port 3123 --https --cert ./valyrian.crt --key ./valyrian.key",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "xxbuild": "tsc -p src/",
    "xxbuild:watch": "tsc -p src/ -w",
    "xxbuild:e2e": "tsc -p e2e/",
    "xxserve": "lite-server -c=bs-config.json",
    "xxserve:e2e": "lite-server -c=bs-config.e2e.json",
    "xxprestart": "npm run build",
    "xxstart": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "xxpree2e": "npm run build:e2e",
    "xxe2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "xxpreprotractor": "webdriver-manager update",
    "xxprotractor": "protractor protractor.config.js",
    "xxpretest": "npm run build",
    "xxtest": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "xxpretest:once": "npm run build",
    "xxtest:once": "karma start karma.conf.js --single-run",
    "xxlint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.3.5",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.3.5",
    "@angular/compiler": "^4.3.5",
    "@angular/core": "^4.3.5",
    "@angular/forms": "^4.3.5",
    "@angular/http": "^4.3.5",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.5",
    "@angular/platform-browser-dynamic": "^4.3.5",
    "@angular/router": "^4.3.5",
    "angular-in-memory-web-api": "^0.3.2",
    "core-js": "^2.5.0",
    "date-fns": "^1.28.5",
    "hammerjs": "^2.0.8",
    "masonry-layout": "^4.2.0",
    "rrule": "^2.2.0",
    "rxjs": "^5.4.3",
    "typings": "^2.1.1",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.24",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.2.3",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "~2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "tslint": "^5.6.0",
    "typescript": "^2.5.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "repository": {}
}
